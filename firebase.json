{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "api/fulldatabase",
        "function": "fulldatabase"
      },
      {
        "source": "api/feedback",
        "function": "feedback"
      }
    ]
  },

  "emulators": {
    "hosting": {
      "port": "8000"
    }
  }
}
