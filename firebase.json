{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "api/fulldatabase",
        "function": "fulldatabase"
      },
      {
        "source": "api/feedback",
        "function": "feedback"
      }
    ]
  }
}
